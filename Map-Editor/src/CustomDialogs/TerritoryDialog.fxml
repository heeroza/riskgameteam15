    <HBox layoutX="28.0" layoutY="22.0" prefHeight="46.0" prefWidth="272.0">
        <children>
            <TextField fx:id="nameTextField" promptText="Name" />
            <ComboBox fx:id="ContinentComboBox" prefWidth="150.0" promptText="Continent" />
